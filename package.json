{
  "name": "node-verifier",
  "version": "0.3.1",
  "description": "extensible value validator",
  "main": "index.js",
  "scripts": {
    "test_jshint": "node_modules/.bin/grunt jshint",
    "test_unit": "node_modules/.bin/nodeunit tests/",
    "test_unit_cov": "NODEVERIFIER_COV=1 node_modules/.bin/nodeunit --reporter=lcov tests/",
    "jscov": "node_modules/.bin/jscoverage lib",
    "test": "npm run test_jshint && npm run test_unit",
    "coveralls": "npm run jscov && npm run test_unit_cov | node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aliaksandr-pasynkau/node-verifier.git"
  },
  "keywords": [
    "node",
    "validator",
    "verifier",
    "validate",
    "verify",
    "check",
    "value",
    "inspect"
  ],
  "author": "aliaksandr-pasynkau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aliaksandr-pasynkau/node-verifier/issues"
  },
  "homepage": "https://github.com/aliaksandr-pasynkau/node-verifier",
  "dependencies": {
    "lodash": "~2.4.1",
    "email-validator": "^1.0.1"
  },
  "devDependencies": {
    "async": "~0.9.0",
    "colors": "^1.0.3",
    "nodeunit": "~0.9.0",
    "grunt": "~0.4.5",
    "grunto": "~1.2.1",
    "grunt-contrib-jshint": "~0.10.0",
    "jscoverage": "^0.5.9",
    "coveralls": "*",
    "grunt-cli": "^0.1.13"
  }
}
